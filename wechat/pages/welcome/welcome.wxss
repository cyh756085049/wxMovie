/* pages/welcome/welcome.wxss */
.container{
  display: flex;
  flex-direction: column;
  align-items: center;
}
page{
  background-color: #b3d4db;
}
.user{
  width: 200rpx;
  height: 200rpx;
  margin-top: 100rpx;
  border-radius: 50%;
}
.name{
  font-size: 32rpx;
  font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
  font-weight: bold;
  margin-top: 160rpx;
}
.moto{
  font-size: 22rpx;
  font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
  font-weight: bold;
  line-height: 80rpx;
}
.moto-container{
  margin-top: 200rpx;
  border: 1px solid #405f80;
  width: 200rpx;
  height: 80rpx;
  border-radius: 5rpx;
  text-align: center;

}